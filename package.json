        "laravel-mix": "^2.1.14",
        "lodash": "^4.17.11"
    },
    "dependencies": {
        "ajv": "^6.5.4",
        "imagemin": "^6.0.0",
        "img-loader": "^3.0.1",
        "laravel-echo": "^1.4.1",
        "pusher-js": "^4.3.1",
        "vue": "^2.5.17",
        "vue-barcode": "^1.1.0",
        "vue-select": "^2.5.1"
    }
